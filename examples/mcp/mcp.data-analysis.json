{
  "mcpServers": {
    "sqlite": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/database-server",
        "./data.db"
      ]
    },
    "sqlite-explorer": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "sqlite-explorer-fastmcp"
      ]
    },
    "csv-processor": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-csv-processor"
      ]
    },
    "json-processor": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-json-tools"
      ]
    },
    "data-exploration": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-data-explorer"
      ]
    },
    "excel2json": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-excel-converter"
      ]
    },
    "chart": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-chart-generator"
      ]
    },
    "calculator": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@wrtnlabs/calculator-mcp"
      ]
    }
  }
}
