{
  "mcpServers": {
    "docker": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@docker/mcp-server"
      ]
    },
    "terraform": {
      "type": "stdio",
      "command": "npx",
      "args": [
