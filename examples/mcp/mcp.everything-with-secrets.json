{
  "mcpServers": {
    "docker": {
      "command": "npx",
      "args": ["-y", "@docker/mcp-server"]
    },
    "terraform": {
      "command": "npx",
      "args": ["-y", "@hashicorp/terraform-mcp-server"]
    },
    "kubernetes": {
      "command": "npx",
      "args": ["-y", "mcp-kubernetes-server"]
    },
    "git": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-git", "."]
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"]
    },
    "sqlite": {
      "command": "npx",
      "args": ["-y", "@executeautomation/database-server", "./data.db"]
    },
    "hackernews": {
      "command": "npx",
      "args": ["-y", "mcp-hn"]
    },
    "web-search": {
      "command": "npx",
      "args": ["-y", "mcp-web-search"]
    },
    "web-scraper": {
      "command": "npx",
      "args": ["-y", "mcp-web-scraper"]
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "csv-processor": {
      "command": "npx",
      "args": ["-y", "mcp-csv-processor"]
    },
    "json-tools": {
      "command": "npx",
      "args": ["-y", "mcp-json-tools"]
    },
    "excel-converter": {
      "command": "npx",
      "args": ["-y", "mcp-excel-converter"]
    },
    "markitdown": {
      "command": "npx",
      "args": ["-y", "markitdown-mcp-server"]
    },
    "pandoc": {
      "command": "npx",
      "args": ["-y", "mcp-pandoc"]
    },
    "pdf-processor": {
      "command": "npx",
      "args": ["-y", "mcp-pdf-tools"]
    },
    "ssh-tools": {
      "command": "npx",
      "args": ["-y", "mcp-ssh-tools", "localhost"]
    },
    "terminal": {
      "command": "npx",
      "args": ["-y", "mcp-terminal-server"]
    },
    "system-monitor": {
      "command": "npx",
      "args": ["-y", "mcp-system-monitor"]
    },
    "browser-automation": {
      "command": "npx",
      "args": ["-y", "browser-mcp"]
    },
    "python-sandbox": {
      "command": "npx",
      "args": ["-y", "mcp-python-sandbox"]
    },
    "javascript-sandbox": {
      "command": "npx",
      "args": ["-y", "mcp-js-sandbox"]
    },
    "time": {
      "command": "uvx",
      "args": ["mcp-server-time"]
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "everything": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-everything"]
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@github/github-mcp-server"],
      "env": {
        "GITHUB_TOKEN": "YOUR_GITHUB_TOKEN_HERE"
      }
    },
    "slack": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-slack"],
      "env": {
        "SLACK_BOT_TOKEN": "xoxb-YOUR-SLACK-BOT-TOKEN-HERE",
        "SLACK_TEAM_ID": "YOUR_SLACK_TEAM_ID_HERE"
      }
    },
    "notion": {
      "command": "npx",
      "args": ["-y", "mcp-notion-server"],
      "env": {
        "NOTION_API_KEY": "YOUR_NOTION_INTEGRATION_TOKEN_HERE"
      }
    },
    "linear": {
      "command": "npx",
      "args": ["-y", "mcp-linear-server"],
      "env": {
        "LINEAR_API_KEY": "YOUR_LINEAR_API_KEY_HERE"
      }
    },
    "jira": {
      "command": "npx",
      "args": ["-y", "mcp-atlassian-server"],
      "env": {
        "JIRA_HOST": "https://your-company.atlassian.net",
        "JIRA_EMAIL": "your-email@company.com",
        "JIRA_API_TOKEN": "YOUR_JIRA_API_TOKEN_HERE"
      }
    },
    "asana": {
      "command": "npx",
      "args": ["-y", "mcp-asana-server"],
      "env": {
        "ASANA_ACCESS_TOKEN": "YOUR_ASANA_PERSONAL_ACCESS_TOKEN_HERE"
      }
    },
    "salesforce": {
      "command": "npx",
      "args": ["-y", "mcp-salesforce-server"],
      "env": {
        "SALESFORCE_CLIENT_ID": "YOUR_SALESFORCE_CONSUMER_KEY_HERE",
        "SALESFORCE_CLIENT_SECRET": "YOUR_SALESFORCE_CONSUMER_SECRET_HERE",
        "SALESFORCE_USERNAME": "your-salesforce-username@company.com",
        "SALESFORCE_PASSWORD": "YOUR_SALESFORCE_PASSWORD_HERE"
      }
    },
    "hubspot": {
      "command": "npx",
      "args": ["-y", "mcp-hubspot-server"],
      "env": {
        "HUBSPOT_ACCESS_TOKEN": "YOUR_HUBSPOT_PRIVATE_APP_TOKEN_HERE"
      }
    },
    "intercom": {
      "command": "npx",
      "args": ["-y", "mcp-intercom-server"],
      "env": {
        "INTERCOM_ACCESS_TOKEN": "YOUR_INTERCOM_ACCESS_TOKEN_HERE"
      }
    },
    "google-drive": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-googledrive"],
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "/path/to/your/service-account-key.json"
      }
    },
    "gmail": {
      "command": "npx",
      "args": ["-y", "mcp-gmail-server"],
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "/path/to/your/service-account-key.json"
      }
    },
    "postgresql": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-postgres"],
      "env": {
        "POSTGRES_CONNECTION_STRING": "postgresql://username:password@localhost:5432/database_name"
      }
    },
    "mongodb": {
      "command": "npx",
      "args": ["-y", "mcp-mongodb-server"],
      "env": {
        "MONGODB_CONNECTION_STRING": "mongodb://username:password@localhost:27017/database_name"
      }
    },
    "redis": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-redis"],
      "env": {
        "REDIS_URL": "redis://username:password@localhost:6379"
      }
    },
    "stripe": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-stripe"],
      "env": {
        "STRIPE_SECRET_KEY": "sk_test_YOUR_STRIPE_SECRET_KEY_HERE"
      }
    },
    "paypal": {
      "command": "npx",
      "args": ["-y", "mcp-paypal-server"],
      "env": {
        "PAYPAL_CLIENT_ID": "YOUR_PAYPAL_CLIENT_ID_HERE",
        "PAYPAL_CLIENT_SECRET": "YOUR_PAYPAL_CLIENT_SECRET_HERE",
        "PAYPAL_MODE": "sandbox"
      }
    },
    "shopify": {
      "command": "npx",
      "args": ["-y", "mcp-shopify-server"],
      "env": {
        "SHOPIFY_STORE_URL": "https://your-store.myshopify.com",
        "SHOPIFY_ACCESS_TOKEN": "YOUR_SHOPIFY_PRIVATE_APP_ACCESS_TOKEN_HERE"
      }
    },
    "openai": {
      "command": "npx",
      "args": ["-y", "mcp-openai-server"],
      "env": {
        "OPENAI_API_KEY": "sk-YOUR_OPENAI_API_KEY_HERE"
      }
    },
    "anthropic": {
      "command": "npx",
      "args": ["-y", "mcp-anthropic-server"],
      "env": {
        "ANTHROPIC_API_KEY": "sk-ant-YOUR_ANTHROPIC_API_KEY_HERE"
      }
    },
    "google-analytics": {
      "command": "npx",
      "args": ["-y", "mcp-google-analytics-server"],
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "/path/to/your/service-account-key.json",
        "GA_PROPERTY_ID": "YOUR_GA4_PROPERTY_ID_HERE"
      }
    },
    "sentry": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sentry"],
      "env": {
        "SENTRY_AUTH_TOKEN": "YOUR_SENTRY_AUTH_TOKEN_HERE",
        "SENTRY_ORG": "your-sentry-organization",
        "SENTRY_PROJECT": "your-sentry-project"
      }
    },
    "aws-lambda": {
      "command": "npx",
      "args": ["-y", "mcp-aws-lambda-server"],
      "env": {
        "AWS_ACCESS_KEY_ID": "YOUR_AWS_ACCESS_KEY_ID_HERE",
        "AWS_SECRET_ACCESS_KEY": "YOUR_AWS_SECRET_ACCESS_KEY_HERE",
        "AWS_REGION": "us-east-1"
      }
    },
    "aws-s3": {
      "command": "npx",
      "args": ["-y", "mcp-aws-s3-server"],
      "env": {
        "AWS_ACCESS_KEY_ID": "YOUR_AWS_ACCESS_KEY_ID_HERE",
        "AWS_SECRET_ACCESS_KEY": "YOUR_AWS_SECRET_ACCESS_KEY_HERE",
        "AWS_REGION": "us-east-1"
      }
    },
    "twitter": {
      "command": "npx",
      "args": ["-y", "mcp-twitter"],
      "env": {
        "TWITTER_BEARER_TOKEN": "YOUR_TWITTER_BEARER_TOKEN_HERE",
        "TWITTER_API_KEY": "YOUR_TWITTER_API_KEY_HERE",
        "TWITTER_API_SECRET": "YOUR_TWITTER_API_SECRET_HERE"
      }
    },
    "discord": {
      "command": "npx",
      "args": ["-y", "mcp-discord-server"],
      "env": {
        "DISCORD_BOT_TOKEN": "YOUR_DISCORD_BOT_TOKEN_HERE"
      }
    }
  }
}